using System.Collections;
using System.Collections.Generic;
using UnityEngine;

    public class PlayerController : MonoBehaviour
    {
        public float movePower = 10f; //Handles the movement speed
        public float KickBoardMovePower = 15f;//Unused Function
        public float jumpPower = 20f; //Set Gravity Scale in Rigidbody2D Component to 5

        
        private Rigidbody2D rb; 
        private Animator anim; //Gets the animations
        Vector3 movement;
        private int direction = 1;
        bool isJumping = false;
        private bool alive = true;
        private bool isKickboard = false;//Ignore unused function

        public Transform attackPoint;
        public float attackRange = 0.5f;
        public LayerMask enemyLayers; //Gets layers tagged "Enemy"
        public int attackDamage = 40;
        public float attackRate = 3f;
        float nextAttackTime = 0f;
        public int maxHealth = 100;
        int currentHealth;

        private Transform spawnPoint;
        private Transform playerPos;
        public float Timer = 5;
        public bool timerIsRunning = false;

        



        // Start is called before the first frame update
        void Start()
        {
            rb = GetComponent<Rigidbody2D>();
            anim = GetComponent<Animator>();
            currentHealth = maxHealth;

            playerPos = GameObject.FindGameObjectWithTag("Player").transform;
            spawnPoint = GameObject.FindGameObjectWithTag("Respawn").transform;


    }

        private void Update()
        {
            //Ignore unused function at the moment
            if (alive)//Executes only if player is alive
            { 
                Attack();
                Jump();
                Run();

            } 
            else
            {
                Restart();
            }
          
        }
        void OnTriggerEnter2D(Collider2D other)//Handles player behaviours
        {
            anim.SetBool("isJump", false);
            if (other.gameObject.CompareTag("DeathHazards"))//If player comes into contact with objects tagged DeathHazards, Player will die.
            {
                Die();
            }
        }
        void KickBoard(){}//Ignore Unused Function

        void Run()
        {
            if (!isKickboard)//Do not remove
            {
                Vector3 moveVelocity = Vector3.zero;
                anim.SetBool("isRun", false);


                if (Input.GetAxisRaw("Horizontal") < 0)
                {
                    direction = -1;
                    moveVelocity = Vector3.left;

                    transform.localScale = new Vector3(direction, 1, 1);
                    if (!anim.GetBool("isJump"))
                        anim.SetBool("isRun", true);

                }
                if (Input.GetAxisRaw("Horizontal") > 0)
                {
                    direction = 1;
                    moveVelocity = Vector3.right;

                    transform.localScale = new Vector3(direction, 1, 1);
                    if (!anim.GetBool("isJump"))
                        anim.SetBool("isRun", true);

                }
                transform.position += moveVelocity * movePower * Time.deltaTime;

            }
            if (isKickboard)
            {
                Vector3 moveVelocity = Vector3.zero;
                if (Input.GetAxisRaw("Horizontal") < 0)
                {
                    direction = -1;
                    moveVelocity = Vector3.left;

                    transform.localScale = new Vector3(direction, 1, 1);
                }
                if (Input.GetAxisRaw("Horizontal") > 0)
                {
                    direction = 1;
                    moveVelocity = Vector3.right;

                    transform.localScale = new Vector3(direction, 1, 1);
                }
                transform.position += moveVelocity * KickBoardMovePower * Time.deltaTime;
            }
        }
        void Jump()//Jump function
        {
            if ((Input.GetButtonDown("Jump") || Input.GetAxisRaw("Vertical") > 0)
            && !anim.GetBool("isJump"))
            {
                isJumping = true;
                anim.SetBool("isJump", true);
            }
            if (!isJumping)
            {
                return;
            }

            rb.velocity = Vector2.zero;

            Vector2 jumpVelocity = new Vector2(0, jumpPower);
            rb.AddForce(jumpVelocity, ForceMode2D.Impulse);

            isJumping = false;
        }
        void Attack()//attack function
        {
            if(Time.time >= nextAttackTime){

            if (Input.GetKeyDown(KeyCode.Alpha1))
            {
                anim.SetTrigger("attack");
                nextAttackTime = Time.time + 1f / attackRate;

                Collider2D[] hitEnemies = Physics2D.OverlapCircleAll(attackPoint.position, attackRange, enemyLayers);

                foreach(Collider2D Enemy in hitEnemies)
                {
                    Enemy.GetComponent<EnemyAI>().TakeDamage(attackDamage);
                    Enemy.GetComponent<CombatReady>().TakeDamage(attackDamage);
                }
            }

            }
        }
        void Hurt()//Hurt animation
        {
           
            anim.SetTrigger("hurt");
            if (direction == 1)
                rb.AddForce(new Vector2(-5f, 1f), ForceMode2D.Impulse);
            else
                rb.AddForce(new Vector2(5f, 1f), ForceMode2D.Impulse);
            
        }

        public void GetDamage(int damage)//Take damage function
        {
            if(currentHealth > 0){
            currentHealth -= damage;
            anim.SetTrigger("hurt");

            if(currentHealth <= 0)
            {
                Die();
                Timer -= Time.deltaTime;
                if (Timer < 0)
                {
                    Respawn();
                }
                
            }
        }

        }
        void Die()
        {
            alive = false;
            Debug.Log("You Died.");
            anim.SetBool("isKickBoard", false);
            anim.SetTrigger("die");
            Timer -= Time.deltaTime;
            
            
        }
        
        void Restart()//Restart function (Ignore for now)
        {
           
            if (Input.GetKeyDown(KeyCode.R))
            {  
                anim.SetTrigger("idle");
                alive = true;
                currentHealth = maxHealth;
                playerPos.position = new Vector3(spawnPoint.position.x, spawnPoint.position.y, spawnPoint.position.z);
            }

            
            
        }
        void Respawn()
        {
            if(timerIsRunning)
            {
                if (Timer > 0)
                {
                    Timer -= Time.deltaTime;
                }
                else
                {
                    alive = true;
                    anim.SetBool("die", false);
                    
                    Debug.Log("You should be spawning now");
                    timerIsRunning = false;
                }
            }


        

    }

    }


