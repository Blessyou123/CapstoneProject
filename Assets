using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class EnemyAI : MonoBehaviour
{
    
    
    public Transform attackPoint; //Is the point of attack for the character
    public LayerMask playerLayers; //Identifies the layers associated with the Player
    public float attackRange = 0.5f; 
    GameObject Player;
    public float speed;
    public bool moving = true;
    public bool MoveRight;
    private Animator            m_animator;
    private Rigidbody2D         m_body2d;
    private bool                m_grounded = false;
    private bool                m_combatIdle = false;
    private bool                m_isDead = false;
    public float within_range;
    public Animator animator;
    public int maxHealth = 100;
    int currentHealth;
    public int attackDamage = 20;
    public float attackRate = 3f;
    float nextAttackTime = 0f;
    public float timer = 5.0f;
    
    

    // Update is called once per frame
    void Start () 
    {
        m_animator = GetComponent<Animator>(); //Gets the animation components
        m_body2d = GetComponent<Rigidbody2D>();//Gets the rigidbody component
        Player = GameObject.FindGameObjectWithTag("Player");//Gets the GameObject with the tag "Player"
        currentHealth = maxHealth;//Health
    }
    void Update()
    {
    
        if(currentHealth > 0)
        {
            float dist = Vector3.Distance(transform.position, Player.transform.position);
            if(dist < within_range)
            {
                if(Time.time >= nextAttackTime)
                {

                attackplayer();
                nextAttackTime = Time.time + 1f / attackRate;
                
                }
                             
            }
            else
            {        
                roam();
            }
        }
        else {
            
            Die();
        }
        
        



    }
     
    void OnTriggerEnter2D(Collider2D trig) //Controls enemy behaviors via collisions
    {
        if(currentHealth > 0){
            if(trig.gameObject.CompareTag("EnemyTurn")) //Controls enemy movements
            {

            if(MoveRight)
            {
                MoveRight = false;
            }
            else{
                MoveRight = true;
            }
            } 
            if(trig.gameObject.CompareTag("Player"))//Controls enemy behavior if player collides with enemy
            {            
                attackplayer();
            }
        }
        
        
    }
    
    
    void attackplayer()
    {      
        m_animator.Play("Attack");

        Collider2D[] hitPlayers = Physics2D.OverlapCircleAll(attackPoint.position, attackRange, playerLayers);

        foreach(Collider2D enemy in hitPlayers)//Controls the damage mechanics for player
        {
            enemy.GetComponent<PlayerController>().GetDamage(attackDamage);
        }
             
    }

    void OnDrawGizmosSelected()
    {
        if(attackPoint == null)
            return;

        Gizmos.DrawWireSphere(attackPoint.position, attackRange);
    }
    void roam()//Functions in charge of the roaming mechanic for the enemy
    {
        
        if(MoveRight)
        {
            transform.Translate(2 * Time.deltaTime * speed , 0,0);
            transform.localScale = new Vector2 (-4,4);
            m_animator.SetInteger("AnimState", 2);
        }
        else
        {
            
            transform.Translate(-2 * Time.deltaTime * speed, 0,0);
            transform.localScale = new Vector2 (4,4);
            m_animator.SetInteger("AnimState", 2);
        }
        
        
    }
    public void TakeDamage(int damage)//Damage function for the damage mechanic
    {
        if(currentHealth > 0){
        currentHealth -= damage;

        m_animator.SetTrigger("Hurt");

        if(currentHealth <= 0)
        {
            Die();
        }
        }

    }
    void Die()//Controls enemy death
    {
        
        Debug.Log("Enemy Died");
        m_animator.SetBool("IsDead", true);
        m_animator.SetTrigger("Death");

        timer -= Time.deltaTime;

        if(timer < 0)
        {
        //Action
        m_animator.SetTrigger("Recover");
        currentHealth = 100;
        attackDamage = 60;
        roam();
        timer = 5;
        
        

        }
        
        
    }
   
}

