using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class CombatReady : MonoBehaviour
{

    public Transform attackPoint;
    public LayerMask playerLayers;
    public float attackRange = 0.5f; 
    GameObject Player;
    public float speed;
    private Animator m_animator;
    private Rigidbody2D m_body2d;
    private bool m_grounded = false;
    private bool m_combatIdle = false;
    private bool m_isDead = false;
    public float within_range;
    public Animator animator;
    public int maxHealth = 100;
    int currentHealth;
    public int attackDamage = 20;
    public float attackRate = 3f;
    float nextAttackTime = 0f;
    

    
    void Start () //This executes the code at the start of the run
    {
        m_animator = GetComponent<Animator>(); //Gets the animation compoents
        m_body2d = GetComponent<Rigidbody2D>(); //Gets the Rigid Body which is needed for character movement
        Player = GameObject.FindGameObjectWithTag("Player"); //Looks for the GameObject with the tag "Player"
        currentHealth = maxHealth; //Health of character
    }
    void Update() //Constantly runs throughout the playthrough
    {
        //Executes this line of code for as long as the Current Health of the character is not 0
        if(currentHealth > 0)
        {
            //Controls the range of attack for the enemy/character
            float dist = Vector3.Distance(transform.position, Player.transform.position);
            if(dist < within_range)
            {
                if(Time.time >= nextAttackTime)
                {

                attackplayer();
                nextAttackTime = Time.time + 1f / attackRate;
                
                }
                             
            }
        }
        else { //If Current Health is 0, character dies.
            
            Die();
        }
        
    }
     
    void OnTriggerEnter2D(Collider2D trig) //This handles the actions of the enemy once they come into contact with the player
    {
        if(currentHealth > 0){
          
            if(trig.gameObject.CompareTag("Player"))
            {            
                attackplayer();
            }
        }
        
        
    }
    //Method for attackplayer()
    void attackplayer()
    {      
        m_animator.Play("Attack"); //Plays animation

        Collider2D[] hitPlayers = Physics2D.OverlapCircleAll(attackPoint.position, attackRange, playerLayers); //Controls range of attack

        foreach(Collider2D enemy in hitPlayers)//If the attack reaches the player. Player will take damage accordingly
        {
            enemy.GetComponent<PlayerController>().GetDamage(attackDamage);
        }
             
    }

    void OnDrawGizmosSelected() //Shows the range of attack
    {
        if(attackPoint == null)
            return;

        Gizmos.DrawWireSphere(attackPoint.position, attackRange);
    }
    void roam()
    {
        
        
    }
    public void TakeDamage(int damage)//Controls the taking of damage for the player and enemy
    {
        if(currentHealth > 0){
        currentHealth -= damage;

        m_animator.SetTrigger("Hurt");

        if(currentHealth <= 0) //If Health drops to 0 or lower. Character dies
        {
            Die();
        }
        }

    }
    void Die()
    {
        Debug.Log("Enemy Died");//Debug Log shows Enemy Died

        m_animator.SetBool("IsDead", true);
        m_animator.SetTrigger("Death");
        this.enabled = false;
        
    }
   
}
